@font-face
  font-family 'icomoon'
  src url('../fonts/icomoon.eot?2pcu8w')
  src url('../fonts/icomoon.eot?2pcu8w#iefix') format('embedded-opentype'),
 url('../fonts/icomoon.ttf?2pcu8w') format('truetype'),
		url('../fonts/icomoon.woff?2pcu8w') format('woff'),
		url('../fonts/icomoon.svg?2pcu8w#icomoon') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'icomoon'
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-menu
  &:before
    content "\e9bd"

.icon-instagram
  &:before
    content "\ea90"

.icon-twitter
  &:before
    content "\ea91"

.icon-github
  &:before
    content "\eab2"

